transform:
    hash (pass)
    to:
    hash:pass

cat uzivatele_hashes.txt | awk 'length($0) > 0 {gsub(/[\(\)]/, "", $2); if ($2 != "") print $1 ":" $2; else print $1 ":" }' >temp
