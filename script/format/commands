transform:
    hash (pass)
    to:
    hash:pass

cat uzivatele_hashes.txt | awk 'length($0) > 0 {gsub(/[\(\)]/, "", $2); if ($2 != "") print $1 ":" $2; else print $1 ":" }' >temp

link all
 find ./rsc/ | grep -P ".*?/edited/.*?/.*?pass.*?.txt" | xargs cat | sort -u  | wc


find ./rsc/ | grep -P ".*?/edited/.*?pass(.[^/])?\.txt" | python3 passlist.py link pass | sort -u | wc
